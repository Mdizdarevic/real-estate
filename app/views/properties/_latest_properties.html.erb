<% @properties.each do |property| %>
        <div class="text-center">
        <div class="row">
        <%= link_to(property_thumbnail(property), property_path(property)) %>
        <h3 class="h5 mt-4"><%= link_to property.address, property_path(property) %><h3>
        <h3>$<%= property.price %><h3>
        </div>
        </div>
<% end %>
