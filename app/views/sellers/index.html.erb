<h1 class="h3">Sellers</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Account ID</th>
      <th>Seller Name</th>
      <th>Property Responsible For</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
    <tr>
      <td><%= property.account_id %></td>
      <td><%= property.name %></td>
      <td><%= property.address %></td>
    </tr>
  <% end %>
  </tbody>
</table>
<br/>